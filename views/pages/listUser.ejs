<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body>

<header>
  <%- include('../partials/header'); %>
</header>

<main  class="container">
    <div class="row">
      <div class="col-sm-3">
        <div class="jumbotron">
            <p>This is left side bar</p>
        </div>
      </div>
      <div class="col-sm-9">
        <div class="jumbotron"  id="jumbotron">
            <table style="width: 100%;">
                <tr>
                    <th>UserID</th>
                    <th>Name</th>
                    <th>Phone</th>
                </tr>
                <% for(var i=0; i< data.length; i++) { %>
                    <tr>
                        <td><%= data[i].userID %></td>
                        <td><%= data[i].name %></td>
                        <td><%= data[i].phone %></td>
                </tr>
                <% } %>
            </table>
        </div>
      </div>
    </div>
    </main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>
